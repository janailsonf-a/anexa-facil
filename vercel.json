{
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "framework": "create-react-app"
      }
    }
  ],
  "routes": [
    { "src": "/api/process", "dest": "/api/process.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}