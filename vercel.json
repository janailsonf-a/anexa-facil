{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "framework": "create-react-app" } },
    { "src": "api/process.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/process", "dest": "/api/process.py" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
